<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <title>Sign Up Form</title>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/css/bootstrap.min.css"
        integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
    <link rel="stylesheet" href="/signUp/index.css">
</head>

<body>
    <div class="main">
        <div class="register">
            <h3>Register your Students Here</h3>
            <p style="text-align: center;"><a href="/api/main" class="card-link"><b>Back to Main Page</b></a></p>
            <form action="/api/main/signUp" id="register" method="post">
                <label for="">Name :</label>
                <br>
                <input type="text" name="name" id="name" placeholder="Enter Your Name">
                <br><br>
                <label for="">Email : </label>
                <br>
                <input type="email" name="email" id="name" placeholder="Enter Your Email">
                <br><br>
                <label for="">Registration No :</label>
                <br>
                <input type="text" name="registrationNo" id="name" placeholder="Registration Number?">
                <br><br>
                <label for="">Department</label>
                <br>
                <input list="department" name="department" id="name" placeholder="Select your department.">
                <datalist id="department">
                    <option value="Computer Systems Engineering">
                    <option value="Civil Engineering">
                    <option value="Mechanical Engineering">
                    <option value="Electrial Communication Engineering">
                    <option value="lectrial Power Engineering">
                    <option value="Indistrial Engineering">
                    <option value="Mining Engineering">
                    <option value="Agriculture Engineering">
                    <option value="Computer Science">
                </datalist>
                    <br><br>
                    <label for="">Gender :</label>
                    <br>
                    &nbsp;&nbsp;&nbsp;
                    <input type="radio" value="male" name="gender" id="male">
                    &nbsp;
                    <label for="name">Male</label>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <input type="radio" value="female" name="gender" id="female">
                    &nbsp;
                    <label for="female">Female</label>
                    <br><br>
                    <input type="submit" href="/api/main" name="submit" id="submit">
            </form>
        </div>
    </div>
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
        integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.3/dist/umd/popper.min.js"
        integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/js/bootstrap.min.js"
        integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy"
        crossorigin="anonymous"></script>
</body>

</html>